GET     /vuelos/{promotion}/{origin}/{destination}/{origin}/{destination}/{departureDate}/{returnDate}/{adults}-{children}-{infants}             						FlightsController.index
GET     /vuelos/{promotion}/{origin}/{destination}/{origin}/{destination}/{departureDate}/{returnDate}/{adults}-{children}-{infants}/{cabin}             				FlightsController.index
