# Application dependencies

require:
    - play 1.3.1
    - play -> crud
    - play -> secure
    - docturno -> morphia 1.5.0a
    - com.google.guava -> guava 19.0

repositories:
    - modules:
        type: chain
        using:
            - repoModules:
                type: http
                artifact: "http://sdk.docturno.com/sdk/[module]-[revision].zip"
        contains:
              - docturno -> *